 CellChangeTimes->{{3.64039887856433*^9, 3.6403988795174522`*^9}},
 CellID->1224892054],

Cell[TextData[{
 Cell["   ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"GitReset", "[", 
   RowBox[{
    StyleBox["repo", "TI"], ",", 
    StyleBox["path", "TI"]}], "]"}]], "InlineFormula"],
 " \[LineSeparator]resets the index entry for ",
 Cell[BoxData[
  StyleBox["path", "TI"]], "InlineFormula"],
 " to the current HEAD version.\n",
 Cell["      ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"GitReset", "[", 
   RowBox[{
    StyleBox["repo", "TI"], ",", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox[
       StyleBox["path", "TI"], 
       StyleBox["1", "TR"]], ",", 
      SubscriptBox[
       StyleBox["path", "TI"], 
       StyleBox["2", "TR"]], ",", 
      StyleBox["\[Ellipsis]", "TR"]}], "}"}]}], "]"}]], "InlineFormula"],
 "\[LineSeparator]resets the index entries for ",
 Cell[BoxData[
  SubscriptBox[
   StyleBox["path", "TI"], 
   StyleBox["i", "TI"]]], "InlineFormula"],
 ".\n",
 Cell["      ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"GitReset", "[", 
   RowBox[{
    StyleBox["repo", "TI"], ",", 
    ButtonBox["All",
     BaseStyle->"Link"]}], "]"}]], "InlineFormula"],
 "\[LineSeparator]resets all index entries in ",
 Cell[BoxData[
  StyleBox["repo", "TI"]], "InlineFormula"],
 ".\n",
 Cell["      ", "ModInfo"],
 Cell[BoxData[
  RowBox[{"GitReset", "[", 
   RowBox[{
    StyleBox["repo", "TI"], ",", 
    StyleBox["path", "TI"], ",", 
    StyleBox["ref", "TI"]}], "]"}]], "InlineFormula"],
 "\[LineSeparator]resets the index to use the version of ",
 Cell[BoxData[
  StyleBox["path", "TI"]], "InlineFormula"],
 " found in the commit ",
 Cell[BoxData[
  StyleBox["ref", "TI"]], "InlineFormula"],
 "."
}], "Usage",
 CellChangeTimes->{{3.6403988956363287`*^9, 3.64039889747439*^9}, {
  3.6403991254702234`*^9, 3.640399173114845*^9}, {3.6403992291083536`*^9, 
